{
  "name": "mycloud",
  "version": "1.0.0",
  "description": "Modern self-hosted cloud storage solution",
  "main": "server/index.js",
  "scripts": {
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "server": "nodemon server/index.js",
    "client": "cd client && npm run dev",
    "build": "cd client && npm run build",
    "start": "node server/index.js",
    "init-db": "node server/scripts/initDb.js",
    "generate-icons": "node generate-icons.js",
    "android": "npx cap sync android && npx cap open android",
    "ios": "npx cap sync ios && npx cap open ios",
    "sync": "npx cap sync"
  },
  "keywords": [
    "cloud",
    "storage",
    "file-sharing"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "adm-zip": "^0.5.10",
    "archiver": "^6.0.2",
    "bcryptjs": "^2.4.3",
    "compression": "^1.8.1",
    "cors": "^2.8.6",
    "dotenv": "^16.6.1",
    "express": "^4.22.1",
    "express-rate-limit": "^7.5.1",
    "helmet": "^7.2.0",
    "jsonwebtoken": "^9.0.3",
    "mime-types": "^2.1.35",
    "multer": "^1.4.5-lts.2",
    "sqlite3": "^5.1.7"
  },
  "devDependencies": {
    "@capacitor/cli": "^5.5.1",
    "concurrently": "^8.2.2",
    "nodemon": "^3.0.2",
    "sharp": "^0.33.5"
  },
  "overrides": {
    "tar": "^7.5.7",
    "cacache": "^19.0.1",
    "make-fetch-happen": "^14.0.1"
  }
}
